{
  "name": "pngtomaskedjpegsvg",
  "version": "0.0.0",
  "description": "Convert alpha PNGs to SVG with embedded JPEG",
  "main": "index.js",
  "scripts": {
    "build": "babel src --extensions=\".js,.ts\" --out-dir dist --source-maps",
    "check-types": "tsc",
    "postinstall": "patch-package",
    "postversion": "git push --follow-tags",
    "start": "node -r ./babel-register.js ./src/index.ts",
    "test": "npm-run-all -p check-types"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nhardy/pngtomaskedjpegsvg.git"
  },
  "keywords": [
    "png",
    "svg",
    "jpeg",
    "alpha",
    "transparent",
    "transparency",
    "convert",
    "mask"
  ],
  "author": "Nathan Hardy",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/register": "^7.10.3",
    "@types/common-tags": "^1.8.0",
    "@types/imagemagick": "0.0.30",
    "@types/lodash": "^4.14.157",
    "@types/node": "^14.0.14",
    "@types/tmp": "^0.2.0",
    "@types/uuid": "^8.0.0",
    "@types/yargs": "^15.0.5",
    "app-module-path": "^2.2.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^7.3.1",
    "eslint-config-nhardy": "^0.15.1",
    "i": "^0.3.6",
    "npm": "^6.14.6",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.2.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.3",
    "common-tags": "^1.8.0",
    "imagemagick": "^0.1.3",
    "imagemagick-convert": "^1.0.3",
    "lodash": "^4.17.15",
    "tmp-promise": "^3.0.2",
    "uuid": "^8.2.0",
    "yargs": "^15.3.1"
  }
}
